    </TLINE>
    <TLINE>
     <TDLINE>,,&amp;SBOOK-FORCURAM&amp;,,&amp;SBOOK-FORCURKEY&amp;,,&amp;SPFLI-CITYTO(8)&amp;</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>/:</TDFORMAT>
     <TDLINE>ENDIF</TDLINE>
    </TLINE>
    <TLINE>
     <TDFORMAT>/E</TDFORMAT>
     <TDLINE>SUM</TDLINE>
